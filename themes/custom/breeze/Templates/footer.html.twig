<script>
	// Get the select boxes and checkboxes
	const categorySelect = document.getElementById("category");
	const brandSelect = document.getElementById("brand");
	const priceSelect = document.getElementById("price");
	const ratingSelect = document.getElementById("rating");
	const fuelsSelect = document.getElementById("car-fuels");
	const featuresCheckboxes = document.querySelectorAll('input[type="checkbox"]');

	// Get all the products
	const products = document.querySelectorAll(".product");

	// Add event listeners to the select boxes and checkboxes
	categorySelect.addEventListener("change", filterProducts);
	brandSelect.addEventListener("change", filterProducts);
	priceSelect.addEventListener("change", filterProducts);
	ratingSelect.addEventListener("change", filterProducts);
	fuelsSelect.addEventListener("change", filterProducts);
	featuresCheckboxes.forEach((checkbox) => {
		checkbox.addEventListener("change", filterProducts);
	});

	// Define the filterProducts function
	function filterProducts() {
		// Get the selected options
		const selectedCategory = categorySelect.value;
		const selectedBrand = brandSelect.value;
		const selectedPrice = priceSelect.value;
		const selectedRating = ratingSelect.value;
		const selectedFuel = fuelsSelect.value;
		const selectedFeatures = [];
		featuresCheckboxes.forEach((checkbox) => {
			if (checkbox.checked) {
				selectedFeatures.push(checkbox.value);
			}
		});

		// Get the current year
		

		// Loop through all the products and show/hide them based on the selected options
		products.forEach((product) => {
			const categoryMatch = selectedCategory === "" || product.getAttribute("data-category") === selectedCategory;
			const brandMatch = selectedBrand === "" || product.getAttribute("data-brand") === selectedBrand;
			const priceMatch =
				  selectedPrice === "" ||
				  (selectedPrice === "0-100000" && product.getAttribute("data-price") <= 100000) ||
				  (selectedPrice === "100000-500000" && product.getAttribute("data-price") > 100000 && product.getAttribute("data-price") <= 500000) ||
				  (selectedPrice === "500000-1000000" && product.getAttribute("data-price") > 500000 && product.getAttribute("data-price") <= 1000000) ||
				  (selectedPrice === "1000000-2000000" && product.getAttribute("data-price") > 1000000 && product.getAttribute("data-price") <= 2000000) ||
				  (selectedPrice === "2000000-5000000" && product.getAttribute("data-price") > 2000000 && product.getAttribute("data-price") <= 5000000) ||
				  (selectedPrice === "5000000-10000000" && product.getAttribute("data-price") > 5000000 && product.getAttribute("data-price") <= 10000000) ||
				  (selectedPrice === "10000000-20000000" && product.getAttribute("data-price") > 10000000 && product.getAttribute("data-price") <= 20000000) ||
				  (selectedPrice === "20000000-50000000" && product.getAttribute("data-price") > 20000000 && product.getAttribute("data-price") <= 50000000) ||
				  (selectedPrice === "50000000-100000000" && product.getAttribute("data-price") > 50000000 && product.getAttribute("data-price") <= 100000000);

			const ratingMatch = selectedRating === "" || product.getAttribute("data-rating") >= selectedRating;
			const fuelMatch = selectedFuel === "" || product.getAttribute("data-fuel") === selectedFuel;
			const featuresMatch = selectedFeatures.length === 0 || selectedFeatures.some((feature) => product.getAttribute("data-features").includes(feature));

			// Filter based on brand year
			const yeear = new Date().getFullYear();
			
			const currentYear = yeear - 1;
			
			const brandYear = (product.getAttribute("data-brand-year"));

			const yearDifference = currentYear - brandYear;

			const yearMatch = selectedBrand === "" || (selectedBrand === "1" && yearDifference < 1) ||
				  (selectedBrand === "2" && yearDifference < 2) ||
				  (selectedBrand === "3" && yearDifference < 3) ||
				  (selectedBrand === "4" && yearDifference < 5) ||
				  (selectedBrand === "5" && yearDifference < 7) ||
				  (selectedBrand === "6" && yearDifference >= 7);

			console.log(yearMatch);

			if (categoryMatch  && priceMatch && ratingMatch && fuelMatch && featuresMatch && yearMatch) {
				product.style.display = "block";
			} else {
				product.style.display = "none";
			}
		});
	}

</script>
<script>
	// Get the URL parameters
	const urlParams = new URLSearchParams(window.location.search);

	// Pre-select the options based on the URL parameters
	if (urlParams.has('category')) {
		categorySelect.value = urlParams.get('category');
	}
	if (urlParams.has('price')) {
		priceSelect.value = urlParams.get('price');
	}
	if (urlParams.has('features')) {
		const selectedFeatures = urlParams.get('features').split(',');
		featuresCheckboxes.forEach((checkbox) => {
			if (selectedFeatures.includes(checkbox.value)) {
				checkbox.checked = true;
			}
		});
	}

	// Call the filterProducts function to filter the products on page load
	filterProducts();

</script>
